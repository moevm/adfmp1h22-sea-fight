<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <inset>
            <shape>
                <stroke
                    android:color="?colorPrimaryDark"
                    android:width="2dp"/>
            </shape>
        </inset>
    </item>
    <item
        android:top="2dp"
        android:right="0dp"
        android:bottom="0dp"
        android:left="2dp">
        <shape>
            <solid android:color="?colorPrimary"/>
        </shape>
    </item>
</layer-list>
